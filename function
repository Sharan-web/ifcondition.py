def moooo(x,y):
 name=(input("enter your name:"))
 age=int(input("enter your age:"))
 roll_num=int(input("enter your roll_num:"))
 addition=age+roll_num
 print("the sum is:",addition)
 multi=x*y
 print("the multi is:",multi)
 return name,age,roll_num
moooo(5,10)
